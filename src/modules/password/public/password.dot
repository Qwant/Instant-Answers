<link rel="stylesheet" href="css/password.css" type="text/css">
<script src="javascript/password.js"></script>
<div class="ia__password__body">
	<div class="ia__password__content">
		<div class="ia__password__line" style="font-weight: bold">Size :<br></div>
		<div class="ia__password__line" style="background: white">{{= it.data.size }}<br></div>
		<div class="ia__password__line" style="font-weight: bold">Password :<br></div>
		<div class="ia__password__line" style="background: white">
			{{~ it.data.password:pass:i }}
				<nobr>{{= pass }}</nobr>
				<br>
			{{~}}
		</div>
	</div>
	{{? it.data.size > 550}}
		<span class="icon icon-nav-scroll-icon-large" style="font-size: 40px;position: relative; bottom: 27px; left: 50%"></span>
	{{?}}
</div>