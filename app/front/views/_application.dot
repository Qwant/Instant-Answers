<div class="application">
    <div class="wrapper">
        <div class="header">
            <span class="header_mobile_bg header_mobile_bg--classic"></span>
            <div class="header_content header_content--classic">
                <button class="home__snippet__extension header__add-qwant">
                    <span>Installer Qwant</span>
                </button>
                <div class="header__item header__item--login">
                    <svg xmlns="https://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-user">
                        <path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path>
                        <circle cx="12" cy="7" r="4"></circle>
                    </svg>
                    <span class="header__item_label">Connexion</span>
                </div>
                <span class="header__item header__item--bookmarks" title="Favoris" aria-label="bookmarks">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-star">
                        <polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"></polygon>
                    </svg>
                </span>
                <span class="header__item header__user__language country--fr_FR" title="Changer la région de recherche">
                    <span class="header__user__language__icon country__icon"></span>
                </span>
                <span class="header__item header__item--appmenu" title="Menu d'applications" aria-label="appmenu">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-menu">
                        <line x1="3" y1="12" x2="21" y2="12"></line><line x1="3" y1="6" x2="21" y2="6"></line><line x1="3" y1="18" x2="21" y2="18"></line>
                    </svg>
                </span>
            </div>
        </div>
        <div id="main">
            <div class="content">
                <div class="results-view results-view--web">
                    <div class="result-list__top">
                        <div class="result-list__search-holder">
                            <div class="search_bar">
                                <div class="search_bar__form">
                                    <a href="/" class="search_bar__form__logo">
                                        <img alt="logo" src="img/v4/qwant-logo-mini.svg" />
                                    </a>
                                    <input class="search_bar__form__placeholder" disabled value spellcheck="false" />
                                    <div class="search_bar__form__icons">
                                        <span class="search_bar__form__reset" title="Effacer la recherche" onclick="document.getElementById('qwant_search').reset(); document.getElementById('search_input').value = '';">
                                            <span class="icon icon-switch-close"></span>
                                        </span>
                                        <svg onclick="document.getElementById('qwant_search').submit();" class="search_bar__form__search" title="Rechercher" width="24" height="24" viewBox="0 0 24 24">
                                            <use xlink:href="/img/v4/icon-spritesheet.svg?#icon-search"></use>
                                        </svg>
                                    </div>
                                    <form action="." id="qwant_search">
                                        <input name="q" id="search_input" class="search_bar__form__input" type="search" autocomplete="off" spellcheck="false" placeholder="Que recherchez-vous ?" value="[[= model.query ]]" aria-label="search_bar" />
                                        <input name="locale" value="fr_fr" type="hidden" />
                                        <input name="lang" value="fr_fr" type="hidden" />
                                    </form>
                                </div>
                            </div>
                        </div>
                        <div class="result-list__carousel">
                            <div class="result-list__carousel__scroll">
                                <a tag="6" class="result-list__carousel-item  result-list__carousel-item--all" href="??q=[[= model.query ]]&t=all">
                                    <svg width="16" height="16" viewBox="0 0 24 24">
                                        <use xlink:href="/img/v4/sidebar-spritesheet.svg?#icon-all"></use>
                                    </svg>
                                    <label class="result-list__carousel__title">Tous</label>
                                </a>
                                <a tag="7" class="result-list__carousel-item result-list__carousel-item--current result-list__carousel-item--web" href="?q=[[= model.query ]]&t=web">
                                    <svg width="16" height="16" viewBox="0 0 24 24">
                                        <use xlink:href="/img/v4/sidebar-spritesheet.svg?#icon-web"></use>
                                    </svg>
                                    <label class="result-list__carousel__title">Web</label>
                                </a>
                                <a tag="8" class="result-list__carousel-item  result-list__carousel-item--news" href="?q=[[= model.query ]]&t=news">
                                    <svg width="16" height="16" viewBox="0 0 24 24">
                                        <use xlink:href="/img/v4/sidebar-spritesheet.svg?#icon-news"></use>
                                    </svg>
                                    <label class="result-list__carousel__title">Actualités</label>
                                </a>
                                <a tag="9" class="result-list__carousel-item  result-list__carousel-item--social" href="?q=[[= model.query ]]&t=social">
                                    <svg width="16" height="16" viewBox="0 0 24 24">
                                        <use xlink:href="/img/v4/sidebar-spritesheet.svg?#icon-social"></use>
                                    </svg>
                                    <label class="result-list__carousel__title">Social</label>
                                </a>
                                <a tag="10" class="result-list__carousel-item  result-list__carousel-item--images" href="?q=[[= model.query ]]&t=images">
                                    <svg width="16" height="16" viewBox="0 0 24 24">
                                        <use xlink:href="/img/v4/sidebar-spritesheet.svg?#icon-images"></use>
                                    </svg>
                                    <label class="result-list__carousel__title">Images</label>
                                </a>
                                <a tag="11" class="result-list__carousel-item  result-list__carousel-item--videos" href="?q=[[= model.query ]]&t=videos">
                                    <svg width="16" height="16" viewBox="0 0 24 24">
                                        <use xlink:href="/img/v4/sidebar-spritesheet.svg?#icon-videos"></use>
                                    </svg>
                                    <label class="result-list__carousel__title">Vidéos</label>
                                </a>
                                <a tag="12" class="result-list__carousel-item  result-list__carousel-item--shopping" href="?q=[[= model.query ]]&t=shopping">
                                    <svg width="16" height="16" viewBox="0 0 24 24">
                                        <use xlink:href="/img/v4/sidebar-spritesheet.svg?#icon-shopping"></use>
                                    </svg>
                                    <label class="result-list__carousel__title">Shopping</label>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="verticals__container">
                        <a class="verticals__item verticals__360 button verticals--selected">
                            <svg class="verticals__toggle__icon verticals__360__logo">
                                <use xlink:href="/img/v4/icon-spritesheet.svg?#logo-2"></use>
                            </svg>
                            Qwant
                        </a>
                        <a class="verticals__item verticals__music button ">
