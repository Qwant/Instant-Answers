                                                        <li class="list__item  selected ">
                                                            <span class="list__item__label selected">Toutes</span>
                                                        </li>
                                                        <li class="list__item ">
                                                            <a id="a_85" class="list__item__label " href="#">Dernières 24 heures</a>
                                                        </li>
                                                        <li class="list__item ">
                                                            <a id="a_86" class="list__item__label " href="#">Dernière semaine</a>
                                                        </li>
                                                        <li class="list__item ">
                                                            <a id="a_87" class="list__item__label " href="#">Dernier mois</a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="result_filters_container__triangle_shadow"></div>
                                    <div class="result_filters_container__triangle"></div>
                                </div>
                            </div>
                            <div class="result_fragment result_fragment--first">
                                [[ var i; ]]
                                [[ for(i = 0; i <= 30; i++) { ]]
