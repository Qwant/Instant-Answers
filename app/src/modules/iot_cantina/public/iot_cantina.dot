<link rel="stylesheet" href="css/iot_cantina.css" type="text/css">

<div class="ia__CantinaIoT">
    {{ var menu = it.data[1].content; }}
    <div class="ia__CantinaIoT__headers">Menu des cantines - {{= menu.sector}} - {{= menu.city}}</div>
    <div class="ia__CantinaIoT__date">
        {{= menu.date_meal}}
    </div>
    <div class="ia__CantinaIoT__content">
        <div class="ia__CantinaIoT__contentCategory">
            <div class="ia__CantinaIoT__contentCategory--dishTitle">
                <div class="ia__CantinaIoT__contentCategory-logoMeals ia__CantinaIoT__contentCategory--entree"></div>
                <div class="ia__CantinaIoT__contentCategory--dishName">Entrée</div>
            </div>
            <div class="ia__CantinaIoT__contentCategory--liner"></div>
            <div class="ia__CantinaIoT__contentCategory--infoMeals">{{= menu.starter_label}}</div>
        </div>
        <div class="ia__CantinaIoT__contentCategory">
            <div class="ia__CantinaIoT__contentCategory--dishTitle">
                <div class="ia__CantinaIoT__contentCategory-logoMeals ia__CantinaIoT__contentCategory--plat"></div>
                <div class="ia__CantinaIoT__contentCategory--dishName">Plat</div>
            </div>
            <div class="ia__CantinaIoT__contentCategory--liner"></div>
            <div class="ia__CantinaIoT__contentCategory--infoMeals">
                <div>{{= menu.garnish_label}}</div>
                <div>-</div>
                <div>{{= menu.main_dish_label}}</div>
            </div>
        </div>
        <div class="ia__CantinaIoT__contentCategory">
            <div class="ia__CantinaIoT__contentCategory--dishTitle">
                <div class="ia__CantinaIoT__contentCategory-logoMeals ia__CantinaIoT__contentCategory--fromage"></div>
                <div class="ia__CantinaIoT__contentCategory--dishName">Fromage</div>
            </div>
            <div class="ia__CantinaIoT__contentCategory--liner"></div>
            <div class="ia__CantinaIoT__contentCategory--infoMeals">{{= menu.dairy_label}}</div>
        </div>
        <div class="ia__CantinaIoT__contentCategory">
            <div class="ia__CantinaIoT__contentCategory--dishTitle">
                <div class="ia__CantinaIoT__contentCategory-logoMeals ia__CantinaIoT__contentCategory--dessert"></div>
                <div class="ia__CantinaIoT__contentCategory--dishName">Dessert</div>
            </div>
            <div class="ia__CantinaIoT__contentCategory--liner"></div>
            <div class="ia__CantinaIoT__contentCategory--infoMeals">{{= menu.dessert_label}}</div>
        </div>
        <div class="ia__CantinaIoT__contentCategory">
            <div class="ia__CantinaIoT__contentCategory--dishTitle">
                <div class="ia__CantinaIoT__contentCategory-logoMeals ia__CantinaIoT__contentCategory--gouter"></div>
                <div class="ia__CantinaIoT__contentCategory--dishName">Goûter</div>
            </div>
            <div class="ia__CantinaIoT__contentCategory--liner"></div>
            <div class="ia__CantinaIoT__contentCategory--infoMeals">{{= menu.snack_label}}</div>
        </div>
    </div>
    <div class="ia__CantinaIoT__source">
        <a class="ia__CantinaIoT__sourceLink" href="https://www.opendatasoft.fr/" target="_blank" rel="noopener noreferrer">via opendatasoft.fr</a>
        <a class="ia__CantinaIoT__sourceStyle" href="https://www.opendatasoft.fr/" target="_blank" rel="noopener noreferrer">>></a>
    </div>
</div>
