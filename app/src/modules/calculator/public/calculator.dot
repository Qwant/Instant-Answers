<link rel="stylesheet" href="css/calculator.css" type="text/css">

<div class="ia__calculator">
    <svg class="ia-calculator-icon" title="" width="24" height="24" viewBox="0 0 24 24">
        <use xlink:href="/img/v4/icon-spritesheet.svg#calc"></use>
    </svg>
    <div class="ia__calculator__container">
        <span class="ia__calculator__query">
            {{= it.query.toLowerCase() }} <!-- prints the original query (ex: 2+2) -->
        </span>
        <span class="ia__calculator__operator">
			&nbsp;=&nbsp;
		</span>
        <span class="ia__calculator__result">
			{{= it.data }} <!-- prints the returned result from the IA solver (ex: 4) -->
		</span>
    </div>
</div>
